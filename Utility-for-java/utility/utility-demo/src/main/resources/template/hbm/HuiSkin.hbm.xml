<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.template.model.huiadmin">
    <class name="com.template.dao.model.huiadmin.HuiSkin" table="t_hui_skin" >
        <id name="id" column="id" unsaved-value="0">
            <generator class="identity" />
        </id>
        <property name="name" column="name" type="java.lang.String" ></property>
        <property name="englishName" column="english_name" type="java.lang.String" ></property>
        <property name="color" column="color" type="java.lang.String" ></property>
    </class>
</hibernate-mapping>